{
  "name": "lecture",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "normalize-path": "^3.0.0",
    "ramda": "^0.26.1",
    "react": "16.8.3",
    "react-native": "0.59.1",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-video": "^4.4.1",
    "react-navigation": "^3.9.1",
    "react-redux": "^7.0.3",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-jest": "^24.5.0",
    "babel-preset-react-native": "4.0.0",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "react-test-renderer": "16.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-unresolved": "error",
      "import/named": "error",
      "import/namespace": "error",
      "import/no-duplicates": "error",
      "no-console": "error",
      "jsx-a11y/accessible-emoji": "off",
      "no-sequences": "off"
    }
  }
}
